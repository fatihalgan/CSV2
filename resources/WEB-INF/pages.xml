 <page conversation-required="true" view-id="/start_customer_session.xhtml">
  <description>Serve Customer: #{currentServedCustomer.customerIdentifier}</description>
  <action execute="#{complaintManagementService.loadComplaintOwner}" if="#{complaintManagementService.complaintOwner == null}"/>
  <action execute="#{complaintManagementService.loadComplaintHistory}" if="#{empty complaintManagementService.complaintHistory}"/>
  <navigation from-action="#{complaintManagementService.rememberInitialComplaintEntry}">
   <redirect view-id="/new_session.xhtml"/>
  </navigation>
 </page>
 <page conversation-required="true" view-id="/end_customer_session.xhtml">
  <description>End Customer Session: #{currentServedCustomer.customerIdentifier}</description>
  <navigation from-action="#{complaintManagementService.saveComplaintEndCustomerSession}">
   <redirect view-id="/main.xhtml"/>
  </navigation>
  <navigation from-action="#{currentServedCustomer.endCustomerSession}">
   <redirect view-id="/main.xhtml"/>
  </navigation>
 </page>
 <page conversation-required="false" view-id="/corporate/start_customer_session.xhtml">
  <description>Serve Customer: #{currentServedCustomer.customerIdentifier}</description>
  <begin-conversation flush-mode="manual" join="true"/>
  <action
   execute="#{complaintManagementService.loadCorporateComplaintOwner}" if="#{currentServedCustomer.customerValid and complaintManagementService.complaintOwner == null}"/>
  <action execute="#{complaintManagementService.loadComplaintHistory}" if="#{empty complaintManagementService.complaintHistory and complaintManagementService.complaintOwner != null}"/>
  <navigation from-action="#{complaintManagementService.rememberInitialComplaintEntry}">
   <redirect view-id="/new_session.xhtml"/>
  </navigation>
  <navigation from-action="#{complaintManagementService.saveComplaintEndCustomerSession}">
   <redirect view-id="/main.xhtml"/>
  </navigation>
 </page>
